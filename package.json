{
  "name": "showcase",
  "version": "0.0.0",
  "main": "index.js",
  "author": "Guillaume Quittet",
  "license": "MIT",
  "scripts": {
    "dev": "parcel src/index.html",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --no-source-maps",
    "start": "parcel serve src/index.html",
    "preserve": "rimraf ./dist",
    "serve": "cross-env NODE_ENV=production parcel serve src/index.html --no-source-maps --open",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist -b master"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.2",
    "cross-env": "^7.0.2",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "node-sass": "^7.0.0",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^2.0.0",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typeit-react": "^0.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
